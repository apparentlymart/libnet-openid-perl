<?_code
{
    use Apache::Request;

    my $r = Apache->request;
    my $ret = "";
    $ret .= "ref = " . ref($r) . "<br />";
    use Data::Dumper;
    $ret .= "<pre>" . Dumper({ $r->args }) . "</pre>";

    my $apr = Apache::Request->new($r);
    my @baz = scalar $apr->param("baz");
    $ret .= "baz: @baz [" . (ref $apr) . "]";
    
    return $ret;

}
_code?>

    
